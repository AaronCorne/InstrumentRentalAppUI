<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CustomSeekBar">
        <item name="android:progressTint">@color/khaki</item>
        <item name="android:thumbTint">@color/teal_700</item>

    </style>
    
    <style name="customBorrowBtn">
        <item name="android:textColor">@color/cream</item>
        <item name="android:background">@color/brown</item>
    </style>

    <style name="customBtn">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@color/blue1</item>
    </style>

    <style name="customText">
        <item name="android:textSize">30sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="customPriceText">
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@color/blue1</item>
    </style>

    <style name="customCreditText">
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@color/green</item>
    </style>

    <style name="saveBtn">
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@color/switchGreen</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="cancelBtn">
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@color/red</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="customDesc">
        <item name="android:textColor">@color/brown</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="customDesc2">
        <item name="android:textColor">@color/brown</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
    </style>




</resources>
